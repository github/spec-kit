{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "User-Endpoints API Response Schema",
  "description": "JSON schema for user-endpoints API endpoint responses",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "description": "Unique identifier"
    },
    "data": {
      "type": "object",
      "description": "Response data payload"
    },
    "meta": {
      "type": "object",
      "properties": {
        "timestamp": {
          "type": "string",
          "format": "date-time"
        },
        "version": {
          "type": "string"
        }
      }
    }
  },
  "required": ["data"]
}